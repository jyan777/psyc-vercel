{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "api/chat.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/chat",
      "dest": "/api/chat.js",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "public/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/public/assets/$1"
    }
  ],
  "github": {
    "silent": true,
    "autoAlias": true,
    "repository": "jyan777/psyc-vercel"
  }
}
